{
    "name": "esign/laravel-database-trigger",
    "description": "Easily create database triggers inside of your Laravel application",
    "keywords": [
        "esign",
        "mysql",
        "trigger",
        "database"
    ],
    "homepage": "https://github.com/esign/laravel-database-trigger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\DatabaseTrigger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\DatabaseTrigger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\DatabaseTrigger\\DatabaseTriggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
